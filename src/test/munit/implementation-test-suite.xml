<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:munit="http://www.mulesoft.org/schema/mule/munit" xmlns:munit-tools="http://www.mulesoft.org/schema/mule/munit-tools"
	xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xsi:schemaLocation="
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/munit http://www.mulesoft.org/schema/mule/munit/current/mule-munit.xsd
		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config name="implementation-test-suite.xml" />
	<munit:test name="implementation-test-suite-getFlightsByIDTest" doc:id="2ebdf0e3-9966-479c-a624-e5cbdfa95e19" description="Test">
	 <munit:behavior>
      
        <set-variable variableName="ID" value="3" doc:name="Set ID Variable"/>
    </munit:behavior>
		<munit:execution >
			<flow-ref doc:name="Flow-ref to getFlightsByID" doc:id="0ac77b56-54f1-488f-8149-a2104b9be16b" name="getFlightsByID"/>
		</munit:execution>
		<munit:validation>
		  <munit-tools:assert-that doc:name="Assert That Payload Size is 1" expression="#[sizeOf(payload)]" is="#[MunitTools::equalTo(1)]" message="The payload must contain exactly 1 object"/>
        <munit-tools:assert-that doc:name="Assert That" expression="#[payload[0].ID]" is="#[MunitTools::equalTo(3)]" message="The payload must contain an object with ID = 3"/>
    </munit:validation>
	</munit:test>
	<munit:test name="implementation-test-suite-getFlightsTest" doc:id="5221fefc-1281-4ed0-ab45-0cd167d165f5" description="Test">
		<munit:execution >
			<flow-ref doc:name="Flow-ref to getFlights" doc:id="01868ed6-f8b2-47ea-bca5-b171805536a1" name="getFlights"/>
		</munit:execution>
		<munit:validation >
		
			<munit-tools:assert-that doc:name="Assert that is JSON Array" doc:id="230172be-a71d-4236-9f1a-bfbe9d0e68b3" is='#[MunitTools::equalTo(true)]' expression="#[output application/java --- payload is Array]"/>
			<munit-tools:assert-that doc:name="Assert that Array size is 11" doc:id="61ccf0db-641e-412b-81d2-13a90ef89486" expression="#[sizeOf(payload)]" is="#[MunitTools::equalTo(11)]" />
		</munit:validation>
	</munit:test>
	


</mule>
